@symbols = '!=' '<=' '>=';
 
@start = expr;
expr = orExpr;
 
orExpr = andExpr orTerm*;

orTerm = 'or'! andExpr {
	PUSH(@([POP() boolValue] || [POP() boolValue]));
};
 
andExpr = relExpr andTerm*;
andTerm = 'and' relExpr;
 
relExpr = callExpr (relOp callExpr)*;
relOp = '<' | '>' | '=' | '!=' | '<=' | '>=';

callExpr = primary ('(' argList? ')')?;
argList = atom (',' atom)*;
 
primary = atom | '(' expr ')';
atom = obj | literal;
 
obj = id member*;
id = Word;
member = '.' id;
 
literal = QuotedString | Number | bool;
bool = 'yes' {POP(); PUSH(@(1));} 
     | 'no'  {POP(); PUSH(@(0));}
	 ;

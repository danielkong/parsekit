@start = stat;

stat =    assign dot { LOG(@"after alt1") }
        | list semi { LOG(@"after alt2") };

assign = { LOG(@"pre action") } list eq list;

list =      lbracket { LOG(@"after rbracket ref") }
            elements { LOG(@"after elements ref") }
            rbracket { LOG(@"after rbracket ref") };

elements = element (comma element { LOG(@"after element ref") })* { LOG(@"after rep") };
element = Number { LOG(@"after const") } | list;

lbracket = '['; // fence
rbracket = ']'!;
comma = ','!;

eq = '=' { LOG(@"after literal") };

dot = '.';
semi = ';';
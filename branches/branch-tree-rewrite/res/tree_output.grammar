@start 
	= foo -> foo 
	| bar -> bar 
	| baz -> baz 
	| bat -> bat
	| multi -> multi
	;
	
foo 
	= Word -> Word
	;

bar 
	= 'baz' -> 'baz'
	;

baz 
	= 'int' Word ';' -> ^('int' Word)
	;

bat 
	= array '[' Number ']' ';' -> ^(array ^('[' Number))
	;

array 
	= 'array' -> 'array'
	;
	
multi
	= var Word (',' Word)* -> ^(var Word+)
	;
	
var = 'var' -> 'var'
	;